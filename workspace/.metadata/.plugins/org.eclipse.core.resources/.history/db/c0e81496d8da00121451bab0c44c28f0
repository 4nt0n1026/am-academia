import br.com.acad.model.avisos.NoticiaCategoria;

public class testeGeral  {
	


	public static void main(String[] args) {
		
		AvisoNoticiaDAO dao = new DAOTeste().getDao();
		
		AvisoNoticia aviso = new AvisoNoticia();
		aviso.setTitulo("djsao");
		aviso.setCategoria(new NoticiaCategoria());
		
		dao.insert(aviso);
		
	}
}

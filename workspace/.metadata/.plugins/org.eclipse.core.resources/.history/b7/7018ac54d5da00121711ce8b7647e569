package br.com.acad.teste;

import javax.ejb.EJB;
import javax.ejb.Singleton;

import br.com.acad.dao.avisos.AvisoNoticiaDAO;

@Singleton
public class DAOTeste {

	@EJB
	private AvisoNoticiaDAO dao;

	public AvisoNoticiaDAO getDao() {
		return dao;
	}

	public void setDao(AvisoNoticiaDAO dao) {
		this.dao = dao;
	}
	
	
	
}
